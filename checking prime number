#include<iostream>
using namespace std;
 void prime(int a)
{
	int k=0;
	for(int i=2;i<a-1;i++)
	{
		if(a%i==0)
		{
			cout<<i<<endl;
			++k;
		}
	}
		cout<<"k="<<k<<endl;
		if(k>0)
		{
			cout<<"composite number";
		}
		else
		{
			cout<<"prime number";
		}
}
int main()
{
	int a;
	cout<<"Enter value of a:";
	cin>>a;
	prime(a);
}
